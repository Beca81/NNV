@model IEnumerable<Proba.Models.Sednice>

@{
    ViewBag.Title = "Index";
}

<h2>Sednice</h2>

<p>
    @Html.ActionLink("Kreiraj novu sednicu", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SednicaID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Datum)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VrstaSednice)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ucionica)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Zapisnik)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Poziv)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SednicaID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Datum)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VrstaSednice)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Ucionica)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Zapisnik)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Poziv)
        </td>
        <td>
            @Html.ActionLink("Izmeni", "Edit", new { id = item.SednicaID }) |
            @Html.ActionLink("Detalji", "Details", new { id = item.SednicaID }) |
            @Html.ActionLink("Obriši", "Delete", new { id = item.SednicaID }) |
            @Html.ActionLink("Prisustvo", "Prisustvo", new { id = item.SednicaID }) |
            @Html.ActionLink("Nov prilog", "../Prilozi/Index", new { id = item.SednicaID })

        </td>
    </tr>
}

</table>
@{
    Html.RenderPartial("~/Views/Shared/_LeftNavbar.cshtml");
}
